{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "reporters": [
    "html",
    "clear-text",
    "progress",
    "dashboard"
  ],
  "testRunner": "jest",
  "coverageAnalysis": "perTest",
  "checkers": [
    "typescript"
  ],
  "ignorePatterns": ["dist/*"],
  "tsconfigFile": "tsconfig.json",
  "thresholds": {
    "high": 30,
    "low": 10,
    "break": 50
  }
}