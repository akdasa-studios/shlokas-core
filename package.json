{
  "name": "@akdasa-studios/shlokas-core",
  "version": "0.0.16",
  "description": "Core library for shlokas app",
  "main": "dist.cjs/index.js",
  "module": "dist.esm/index.js",
  "typings": "dist.cjs/index.d.js",
  "files": [
    "./dist.cjs/**/*",
    "./dist.esm/**/*"
  ],
  "exports": {
    "./*": {
      "types": "./dist.cjs/*/index.d.ts",
      "require": "./dist.cjs/*/index.js",
      "import": "./dist.esm/*/index.js"
    }
  },
  "typesVersions": {
    ">=3.2": {
      "*": [
        "./dist.cjs/index.d.ts"
      ]
    }
  },
  "scripts": {
    "test:unit": "jest",
    "test:mutational": "stryker run",
    "lint": "eslint . --ext .ts",
    "build": "webpack && tsc --emitDeclarationOnly --declaration --out ./dist/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akdasa-studios/shlokas-core.git"
  },
  "author": "Advaita Kṛṣṇa Dāsa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/akdasa-studios/shlokas-core/issues"
  },
  "homepage": "https://github.com/akdasa-studios/shlokas-core#readme",
  "dependencies": {
    "@akdasa-studios/framework": "^0.1.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^6.3.0",
    "@stryker-mutator/jest-runner": "^6.3.0",
    "@stryker-mutator/typescript-checker": "^6.3.0",
    "@types/jest": "^29.2.3",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "jest": "^29.3.1",
    "jest-cucumber": "^3.0.1",
    "resolve-tspaths": "^0.8.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  }
}
